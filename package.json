{
  "name": "KiboApi",
  "version": "1.0.0",
  "description": "KiboApi official repository",
  "main": "app.js",
  "scripts": {
    "start": "cross-env DEBUG=kibopush:* NODE_ENV=development nodemon server/app.js",
    "start:prod": "cross-env NODE_ENV=production node server/app.js",
    "debug": "cross-env DEBUG=express:router:route,express:application,kibopush:* NODE_ENV=development nodemon --trace-sync-io server/app.js",
    "watch": "watchify client/src/app.js -t babelify -o client/public/js/bundle.js --debug --verbose",
    "build:client": "sh build_script",
    "build:react": "echo 'Build reactjs bundle...' && browserify -t [ babelify --presets [ es2015 react ] ] client/src/app.js --debug | exorcist client/public/js/bundle.js.map > client/public/js/bundle.js && uglifycss https://cdn.cloudkibo.com/public/assets/vendors/custom/css/custom-style.css --output client/public/dist/custom-style.min.css https://cdn.cloudkibo.com/public/Bootstrap/dist/css/latestBootstrap.css https://cdn.cloudkibo.com/public/assets/Emoji/picker.css https://cdn.cloudkibo.com/public/assets/dashboard/dashboard.css --output client/public/dist/kibopush.min.css &&echo 'Done compiling reactjs file'",
    "lint": "echo 'Pre-commit checks...' && standard --fix && echo \"No code style errors found.\"",
    "test": "echo \"Code tests are under construction by Sojharo\" && exit 0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cloudkibo/KiboAPI.git"
  },
  "keywords": [
    "kibo",
    "kiboapi"
  ],
  "author": "anisha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Cloudkibo/KiboPush_Support/issues"
  },
  "homepage": "http://www.kibopush.com",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.17.2",
    "composable-middleware": "^0.3.0",
    "compression": "^1.7.0",
    "concurrently": "^3.5.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^2.6.8",
    "errorhandler": "^1.5.0",
    "express": "^4.15.3",
    "express-jsonschema": "^1.1.6",
    "express-jwt": "^5.3.0",
    "helmet": "^3.6.1",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.3.0",
    "lodash": "^4.17.4",
    "lodash.intersection": "^4.4.0",
    "method-override": "^2.3.9",
    "moment": "^2.19.1",
    "moment-range": "^3.0.3",
    "mongoose": "^4.11.4",
    "morgan": "^1.8.2",
    "needle": "^1.6.0",
    "passport": "~0.2.0",
    "passport-facebook": "latest",
    "passport-facebook-extension": "0.0.7",
    "passport-local": "^0.1.6",
    "pug": "^2.0.3",
    "raven": "^2.4.1",
    "react": "^15.6.1",
    "react-addons-pure-render-mixin": "^15.6.0",
    "react-autocomplete": "^1.0.0-rc2",
    "react-bootstrap": "^0.30.8",
    "react-cookie": "^0.4.5",
    "react-dom": "^15.6.1",
    "react-facebook-login": "^3.6.2",
    "react-redux": "^4.1.2",
    "react-router": "^2.6.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.4",
    "react-tooltip": "^3.3.0",
    "redux": "^3.1.5",
    "redux-form": "^7.1.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^1.7.4",
    "socket.io-client": "^1.7.4",
    "socketio-jwt": "^2.3.5",
    "swagger-tools": "^0.10.3",
    "underscore": "^1.8.3",
    "valid-url": "^1.0.9",
    "watch": "^1.0.2",
    "watchify": "^3.9.0",
    "winston": "^2.3.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "body-parser": "^1.17.2",
    "cross-env": "^5.0.1",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.3"
  },
  "standard": {
    "ignore": [
      "client/public"
    ],
    "parser": "babel-eslint"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "documentation/*"
    ],
    "delay": "1000"
  }
}
